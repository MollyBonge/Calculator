local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Fade | FULL RELEASE", HidePremium = false, SaveConfig = true,IntroText = "Fade v1", ConfigFolder = "OrionTest"})

--NOTIFICATION
OrionLib:MakeNotification({
    Name = "Fade",
    Content = "v1 full release",
    Image = "rbxassetid://11948032342",
    Time = 5
})
OrionLib:Init()

--MAIN
local Main = Window:MakeTab({
    Name = "Main",
    Icon = "rbxassetid://4034483344",
    PremiumOnly = false
})

Main:AddButton({
    Name = "useless button (idk why its here)",
    Callback = function()
            OrionLib:MakeNotification({
    Name = "Fade",
    Content = "motherfucker i said its useless",
    Image = "rbxassetid://11948032342",
    Time = 5
})
    end    
})

--PLAYER
